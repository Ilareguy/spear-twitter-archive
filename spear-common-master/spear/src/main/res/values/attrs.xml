<?xml version="1.0" encoding="utf-8"?>
<resources>
    <bool name="isTablet">false</bool>

    <attr name="icon" format="reference"/>
    <attr name="text" format="string"/>
    <attr name="selected" format="boolean" />
    <attr name="textColor" format="color" />
    <attr name="iconTint" format="color" />

    <declare-styleable name="MaterialBanner">
        <attr name="highlightColor" format="color"/>
        <attr name="primaryActionText" format="string"/>
        <attr name="secondaryActionText" format="string"/>
        <attr name="icon" />
        <attr name="text" />
    </declare-styleable>
    <declare-styleable name="OnOffLabelView">
        <attr name="selected" />
        <attr name="icon" />
        <attr name="text" />
        <attr name="textColor" />
        <attr name="iconTint" />
    </declare-styleable>
    <declare-styleable name="MaterialHeader">
        <attr name="text" />
        <attr name="icon" />
        <attr name="textColor" />
        <attr name="iconTint" />
        <attr name="showSeparator" format="boolean" />
    </declare-styleable>
    <declare-styleable name="CheckableImageButton">
        <attr name="regularColor" format="color" />
        <attr name="checkedColor" format="color" />
        <attr name="checked" format="boolean" />
        <attr name="checkable" format="boolean" />
        <attr name="src" format="reference" />
        <attr name="animate" format="boolean" />
    </declare-styleable>
</resources>